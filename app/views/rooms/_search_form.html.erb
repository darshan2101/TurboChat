<%= form_with url: search_rooms_path,
    method: :post,
    id: "room_search_path" do |form|
%>
    <%= form.search_field :name_search, class: "room_search_field" , autocomplete: 'off', placeholder: "Search Room" %>
<% end %>
<div id="search_results">
    <%= render partial: 'rooms/search_results', locals: {rooms: @rooms} %>
</div>